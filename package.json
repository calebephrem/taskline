    "todo manager",
    "todo management"
  ],
  "scripts": {},
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "unique-custom-id": "^3.1.0"
  },
  "license": "MIT"
