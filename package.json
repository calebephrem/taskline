{
  "name": "taskline",
  "displayName": "Taskline",
  "description": "ðŸ‘† Your Clickable Todo Companion in VS Code",
  "icon": "assets/icon.png",
  "version": "1.0.2",
  "main": "./extension.js",
  "publisher": "CalebEphrem",
  "author": {
    "name": "Caleb Ephrem",
    "email": "quantumpandaprog@gmail.com"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calebephrem/taskline"
  },
  "bugs": {
    "url": "https://github.com/calebephrem/taskline/issues",
    "email": "quantumpandaprog@gmail.com"
  },
  "activationEvents": [
    "onLanguage:todo"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "taskline.todoEditor",
